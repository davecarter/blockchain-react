{
  "name": "blockchain-react",
  "version": "0.1.0",
  "description": "A Blockchain Project build with JavaScript and ReactJS",
  "main": "src/index.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack --mode production",
    "start": "./node_modules/.bin/webpack-dev-server --config ./webpack.config.js --mode development",
    "co": "sui-mono commit",
    "now-build": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "start:server": "npx json-server --watch ./src/api.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davecarter/blockchain-react.git"
  },
  "keywords": [
    "Blockchain",
    "javascript",
    "ReactJS"
  ],
  "author": "David Garcia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davecarter/blockchain-react/issues"
  },
  "homepage": "https://github.com/davecarter/blockchain-react#readme",
  "eslintConfig": {
    "extends": [
      "./node_modules/@s-ui/lint/eslintrc.js"
    ]
  },
  "prettier": "./node_modules/@s-ui/lint/.prettierrc.js",
  "stylelint": {
    "extends": "./node_modules/@s-ui/lint/stylelint.config.js"
  },
  "jest": {
    "transform": {
      "^.+\\.[jt]sx?$": "babel-jest"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.4",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.8.3",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.8.3",
    "@babel/preset-env": "7.8.7",
    "@babel/register": "7.10.3",
    "@babel/runtime": "7.8.4",
    "@s-ui/lint": "3.14.0",
    "@testing-library/jest-dom": "5.11.0",
    "@testing-library/react": "10.4.6",
    "babel-jest": "26.1.0",
    "babel-loader": "8.0.5",
    "babel-preset-sui": "3",
    "css-loader": "3.4.2",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "mini-css-extract-plugin": "0.4.1",
    "node-sass": "4.13.1",
    "react-dev-utils": "10.2.0",
    "sass-loader": "8.0.2",
    "webpack": "4.41.6",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3"
  },
  "dependencies": {
    "@babel/compat-data": "^7.8.0",
    "@babel/polyfill": "7.8.3",
    "axios": "0.19.2",
    "classnames": "2.2.6",
    "crypto-js": "4.0.0",
    "firebase": "7.12.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-router-dom": "5.1.2"
  },
  "resolutions": {
    "@babel/preset-env": "7.8.7"
  }
}
